{
  "name": "klg-tracer",
  "version": "3.0.0",
  "description": "tracer log ",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "build": "tsc -d -p ./",
    "test": "DEBUG=PandoraHook:* TS_NODE_CACHE=true mocha -t 100000",
    "prepublishOnly": "npm run build"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/src/.*\\.(test|spec).(ts|tsx|js)$",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,tsx,ts}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageReporters": [
      "json",
      "lcov"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaolalicai/klg-tracer"
  },
  "keywords": [
    "klg"
  ],
  "author": "nick",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "cls": "^0.1.5",
    "cls-hooked": "^4.2.2",
    "cls-mongoose-shim": "0.0.4",
    "dorapan": "^1.0.5",
    "fh-cls-mongoose": "^2.1.1",
    "http": "0.0.0",
    "is-type-of": "^1.2.0",
    "klg-logger": "^1.0.4",
    "klg-tracer-model": "^2.0.0",
    "mixin": "^0.2.0",
    "module-hook": "^2.1.0",
    "mongoose": "^4.13.12",
    "opentracing": "^0.14.3",
    "pandora-hook": "^1.8.24",
    "pandora-hub": "^1.8.15",
    "pandora-metrics": "^1.8.18",
    "parseurl": "^1.3.2",
    "sinon": "^5.0.3",
    "uuid": "^3.2.1",
    "xorshift": "^1.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/koa": "^2.0.45",
    "@types/mocha": "^5.2.0",
    "@types/mongoose": "^5.0.9",
    "@types/node": "^9.6.4",
    "@types/sinon": "^4.3.1",
    "@types/superagent": "^3.5.8",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "mocha": "^5.1.1",
    "mongodb-mock-server": "^1.0.0",
    "nock": "^9.2.5",
    "nyc": "^11.7.1",
    "nyc-ts-patch": "^1.0.0",
    "pandora-env": "^1.7.7",
    "power-assert": "^1.5.0",
    "source-map-support": "^0.5.3",
    "superagent": "^3.8.2",
    "tracer": "^0.8.12",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-klg": "^1.0.4",
    "typescript": "^2.8.1",
    "urllib": "^2.27.0",
    "webstorm-disable-index": "^1.2.0"
  }
}
